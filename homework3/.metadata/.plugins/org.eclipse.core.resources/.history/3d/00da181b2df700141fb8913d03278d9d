
import java.util.Scanner;
public class task3 {
	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in,"UTF-8");


System.out.print("n=");
int n = input.nextInt();
System.out.print("m=");
int m = input.nextInt();
long result = 1;
for (int i = 0; i < m; i++) {
result *= n;
}
System.out.println("n^m = " + result);
}
}
