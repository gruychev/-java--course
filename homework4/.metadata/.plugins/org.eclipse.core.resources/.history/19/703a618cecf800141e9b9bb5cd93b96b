package homework4;


public class task3 {
	public static void main(String[] args) {
		int n=10;		 
	     int[] array = new int[10];
	     for (int i = 0; i < n; i++) {
	    	 array[i] = input.nextInt();
	     }
	    	 	    	 
	     int sum = 0;
	     for (int i = 0; i < n; i++) {
	    	 if(array[i] % 2 == 0){
	    	 sum=sum +array[i];
	    	 }
	     
	     }
	     
	     System.out.print(sum);


}
}