
import java.util.Scanner;

public class task5{

    public static void main(String[] args) {

        Scanner input = new Scanner(System.in,"UTF-8");

        System.out.println("Please enter 1:");
        int a = input.nextInt();
      
         if ( a>=1 && a<=3)
            System.out.println("interval 1");
         else if ( a>=4 && a<=6)
        	 System.out.println("interval 2");
         else if ( a>=7 && a<=9)
        	 System.out.println("interval 3");
                  else   
            System.out.println("Entered numbers are not triangle");
        
    }
}