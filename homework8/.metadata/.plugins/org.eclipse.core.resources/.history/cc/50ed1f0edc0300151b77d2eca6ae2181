package homework8;



public class task7 {
	

	    public static void main(String[] args) {

	        String text = "homework";

	 

	        //System.out.println(reversed);

	    

	 	    

	        StringBuilder sb = new StringBuilder();

	        for (int i = text.length() - 1; i >= 0; i--)

	            sb.append(text.charAt(i));

	        sb.toString();

	    }

	}
