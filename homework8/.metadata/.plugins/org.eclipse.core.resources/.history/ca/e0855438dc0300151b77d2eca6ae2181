package homework8;



public class task7 {
	

	    public static void main(String[] args) {

	        String text = "homework";
      StringBuilder sb = new StringBuilder();

	        for (int i = text.length() - 1; i >= 0; i--){

	            sb.append(text.charAt(i));

	           String ss = sb.toString();
	        
	          System.out.println(ss);
	        }
	    }

	}
